<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.carpedm.sign">

	<select id="sign_in" resultType="MemberDTO">
        select * from member where m_id = #{m_id} and m_pw = #{m_pw}
    </select>
	
	<select id="login_up" resultType="MemberDTO">
		select * from member
	</select>
	
	<select id="find_pw" resultType="MemberDTO">
		select * from member
	</select>

	<select id="find_id_email" resultType="MemberDTO">
		select * from member
	</select>
	
	<select id="find_id_tel" resultType="MemberDTO">
		select * from member
	</select>
</mapper>